<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_assessment"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.example.picos.ui.activity.assessment.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_fillQuestionFragment"
            app:destination="@id/fillQuestionFragment" />
    </fragment>
    <fragment
        android:id="@+id/questionsFragment"
        android:name="com.example.picos.ui.activity.assessment.QuestionsFragment"
        android:label="fragment_questions"
        tools:layout="@layout/fragment_question" >
        <action
            android:id="@+id/action_questionsFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.example.picos.ui.activity.assessment.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" />
    <fragment
        android:id="@+id/ageQuestionFragment"
        android:name="com.example.picos.ui.activity.assessment.AgeQuestionFragment"
        android:label="AgeQuestionFragment" >
        <action
            android:id="@+id/action_ageQuestionFragment_to_questionsFragment"
            app:destination="@id/questionsFragment" />
    </fragment>
    <fragment
        android:id="@+id/YNQuestionFragment"
        android:name="com.example.picos.ui.activity.assessment.YNQuestionFragment"
        android:label="YNQuestionFragment" >
        <action
            android:id="@+id/action_YNQuestionFragment_to_ageQuestionFragment"
            app:destination="@id/ageQuestionFragment" />
    </fragment>
    <fragment
        android:id="@+id/categoricalQuestionFragment"
        android:name="com.example.picos.ui.activity.assessment.CategoricalQuestionFragment"
        android:label="CategoricalQuestionFragment" >
        <action
            android:id="@+id/action_categoricalQuestionFragment_to_YNQuestionFragment"
            app:destination="@id/YNQuestionFragment" />
    </fragment>
    <fragment
        android:id="@+id/fillQuestionFragment"
        android:name="com.example.picos.ui.activity.assessment.FillQuestionFragment"
        android:label="FillQuestionFragment" >
        <action
            android:id="@+id/action_fillQuestionFragment_to_categoricalQuestionFragment"
            app:destination="@id/categoricalQuestionFragment" />
    </fragment>
</navigation>